.style-selector-view {
    /*background-color: aquamarine;*/
    width: 100%;
    height: 330rpx;
}

.style-selector {
    /*background-color: aquamarine;*/
    width: 100%;
    height: 100%;
}

.style-selector-inner {
    /*background-color: aquamarine;*/
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
    overflow: scroll;
}

.preview {
    width: 100%;
    height: 150rpx;
    display: flex;
    flex-direction: row;
}

.preview-item {
    margin: auto;
}

.preview-item-close{
    position: relative;
    width: 150rpx;
    height: 150rpx;
}

.preview-selector {
    position: absolute;
}
.preview-close {
    position: absolute;
    top: 0;
    right: -10rpx;
}

.preview-placeholder {
    height: 152rpx;
    width: 152rpx;
}

.preview-next {
    border: 4rpx dotted #ccc;
    /*background-color: rgba(236, 231, 231, 0.88);*/
    text-align: center;
}
